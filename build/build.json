{
    "package": {
        "name": "spinnaker-deployment-tools",
        "repo": "Spinnaker",
        "subject": "chartboost-bot",
        "desc": "Deployment Tools for Spinnaker",
        "website_url": "www.chartboost.com",
        "issue_tracker_url": "https://github.com/ChartBoost/spinnaker-deployment-tools/issues",
        "vcs_url": "https://github.com/ChartBoost/spinnaker-deployment-tools.git",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "RELEASE.txt",
        "licenses": ["MIT"],
        "public_download_numbers": false,
        "public_stats": false
    },

    "version": {
        "name": "0.5",
        "desc": "Main",
        "released": "2015-01-04",
        "vcs_tag": "0.5",
        "gpgSign": false
    },

	"files": [
		{
			"includePattern": "build/(.*\.deb)", 
			"uploadPattern": "$1",
			"matrixParams": {
				"deb_distribution": "trusty",
				"deb_component": "main",
				"deb_architecture": "amd64"
			}
    }
]
